{
  "name": "eayl-demo",
  "version": "1.0.0",
  "description": "A simple Express.js app with TypeScript",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
